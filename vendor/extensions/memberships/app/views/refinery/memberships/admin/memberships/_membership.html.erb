<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(membership) -%>">
  <span class='title'>
    <%= membership.title %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.memberships_membership_path(membership),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_memberships_admin_membership_path(membership),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.memberships_admin_membership_path(membership),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => membership.title),
        :method => :delete %>
  </span>
</li>
